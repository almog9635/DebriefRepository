type Task {
    id: ID!
    content: String!
    startDate: ZonedDateTime!
    deadline: ZonedDateTime!
    completed: Boolean!
    user: User!
    debrief: Debrief!
    lesson: Lesson
}

input TaskInput {
    id: ID
    content: String
    startDate: ZonedDateTime
    deadline: ZonedDateTime
    completed: Boolean
    user: ID
    debrief: ID
    lesson: ID
}
