type Mutation {
    addDebrief(input: DebriefInput!): Debrief
    updateDebrief(id: ID!, input: DebriefInput!): Debrief
    deleteDebrief(id: ID!): Boolean

    addUser(input: UserInput!): User
    updateUser(input: UserUpdateInput!): User
    deleteUser(id: ID!): Boolean

    addGroup(input: GroupCreate!): Group
    updateGroup(input: GroupUpdate!): Group
    deleteGroup(id: ID!): Boolean
}