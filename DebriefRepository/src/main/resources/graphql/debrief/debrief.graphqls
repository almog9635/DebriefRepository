type Debrief{
    id: ID!
    content: [ContentItem!]!
    date: ZonedDateTime!
    user: User!
    group: Group!
    lessons: [Lesson!]
    missions: [Mission!]!
}

input DebriefInput{
    id : ID
    content: String
    date: ZonedDateTime
    user: ID
    group: GroupInput
    lesson: LessonInput
    mission: MissionInput
}

input DebriefUpdate{
    content: String!
    date: ZonedDateTime!
    user: ID!
    group: String!
    lessons: [ID!]!
    missions: [ID!]!
}
