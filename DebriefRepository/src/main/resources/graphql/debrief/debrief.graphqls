type Debrief{
    id: ID!
    content: [Content!]!
    date: ZonedDateTime!
    user: User!
    group: Group!
    lessons: [Lesson!]
    missions: [Mission!]!
}

input DebriefInput{
    id : ID
    content: [ContentItemInput!]
    date: ZonedDateTime
    user: ID
    group: GroupInput
    lesson: LessonInput
    mission: MissionInput
}

input DebriefUpdate{
    content: [ContentItemInput!]
    date: ZonedDateTime
    user: ID
    group: String
    lessons: [ID!]
    missions: [ID!]
}
