type Debrief{
    id: ID!
    contentItems: [Content!]!
    date: ZonedDateTime!
    user: User!
    group: Group!
    lessons: [Lesson!]
    tasks: [Task!]!
}

input DebriefInput{
    id : ID
    contentItems: ContentInput
    date: ZonedDateTime
    user: ID
    group: GroupInput
    lesson: LessonInput
    tasks: TaskInput
}

input DebriefUpdate{
    id: ID
    contentItems: ContentInput
    date: ZonedDateTime
    user: ID
    group: String
    lessons: [ID!]
    missions: [ID!]
}
