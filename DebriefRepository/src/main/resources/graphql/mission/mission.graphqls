type Mission{
    id: ID!
    content: String!
    startDate: Date!
    deadline: Date!
    user: User!
    debrief: Debrief!
    lesson: Lesson
}

input MissionInput{
    id: ID
    content: String
    startDate: Date
    deadline: Date
    user: UserInput
    debrief: DebriefInput
    lesson: LessonInput
}

input MissionUpdate{
    content: String!
    startDate: Date!
    deadline: Date!
    user: ID!
    debrief: ID!
    lesson: ID
}