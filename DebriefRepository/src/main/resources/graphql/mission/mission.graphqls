type Mission{
    id: ID!
    content: String!
    startDate: Date!
    deadline: Date!
    user: User!
    debrief: Debrief!
    lesson: Lesson
}

input MissionInput{
    content: String!
    date: Date!
    deadline: Date!
    User: ID!
    debrief: DebriefInput!
    lesson: LessonInput
}

input MissionUpdate{
    id: ID!
    content: String
    date: Date
    deadline: Date
    User: ID!
    debrief: DebriefInput!
    lesson: LessonInput
}